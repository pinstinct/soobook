{
  "AWSEBDockerrunVersion": 2,
  "volumes": [
    {
      "name": "soobook-deploy",
      "host": {
        "sourcePath": "/var/app/current/django_app"
      }
    },
    {
      "name": "nginx-proxy-conf",
      "host": {
        "sourcePath": "/var/app/current/proxy/conf.d"
      }
    }
  ],
  "containerDefinitions": [
    {
      "name": "soobook-deploy",
      "essential": true,
      "memory": 512,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 4040
        }
      ]
    }
  ]
}